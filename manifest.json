{
  "manifest_version": 3,
  "name": "Focalize",
  "version": "0.1.0",
  "version_name": "0.1.0-alpha",
  "description": "Extension for Lens Protocol",
  "permissions": [
    "activeTab", "scripting", "storage"
  ],
  "options_page": "src/options/index.html",
  "action": {
    "default_title": "Share on Lens"
  },
  "background": {
    "service_worker": "src/background.ts",
    "type": "module"
  }
}